type Query {
    listMembers: [Member]
    getMemberByPhone(phone: String!): Member
}

type Mutation {
    addUpdateMember(phone: String!, firstName: String!, lastName: String!, email: String, preferredContact: String): Member
    removeMember(phone: String!): SimpleResult
}

type Subscription {
    members: [Member]
}

type Member {
    phone: String!
    firstName: String!
    lastName: String!
    email: String
    preferredContact: String
}

type SimpleResult {
    result: Boolean!
}
